  "scripts": {
    "build": "npm run build:dev",
    "start": "npm run lint && nodemon",
    "start:nest": "npm run lint && nest start --watch",
    "clean:dist": "rimraf dist",
    "build:dev": "npm run clean:dist && webpack --progress --config webpack.config.js",
    "build:qas": "npm run clean:dist && webpack --progress --config webpack.config.js",
    "build:prod": "npm run clean:dist && webpack --progress --config webpack.config.js",
    "generate:docs": "npx compodoc -p tsconfig.json -s",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --ext .ts ./src --fix",
    "lint:report": "eslint --ext .ts ./src -f json -o eslint-report.json || exit 0",
    "prettier:base": "prettier --parser typescript",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.ts\"",
    "prettier:fix": "npm run prettier:base -- --write \"src/**/*.ts\"",
    "test": "npm run lint && jest",
    "test:cov": "npm run lint:report && jest",
    "test:watch": "npm run lint && jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
