  "scripts": {
    "build:css": "yarn extract:foundations && tailwindcss -i src/index.css --output src/tailwind.css",
    "build:dev": "yarn build:css && webpack --env development -c config/webpack/app.config.ts",
    "build": "yarn build:css && webpack --env production -c config/webpack/module.config.ts",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider && yarn dev:css --watch & yarn dev:webpack",
    "dev:webpack": "webpack serve --open --env development -c config/webpack/app.config.ts",
    "dev:css": "yarn extract:foundations && tailwindcss --output src/tailwind.css",
    "extract:foundations": "yarn node scripts/extract-foundations.ts",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint './src/**/*.@(js|jsx|ts|tsx)'",
    "start": "yarn build:css --watch & yarn start",
    "start:module": "webpack --watch --env development -c webpack/module.config.ts",
    "test": "TEST_ENV=\"development\" jest --config ./jest.config.js",
    "test-ci": "jest --coverage --runInBand",
    "messages-check": "yarn messages-extract && git diff --exit-code --quiet lang",
    "messages-compile": "formatjs compile-folder lang langCompiled --ast",
    "messages-extract": "formatjs extract 'src/**/*.ts*' --out-file lang/en-US.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
